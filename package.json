{
  "name": "react-vite-tailwnd-template",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview --port 4173",
    "lint": "eslint src/* --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix src/* --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint"
    }
  },
  "lint-staged": {
    "*.js(?:on)?": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "pnpm lint:fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "4.35.3",
    "axios": "1.5.0",
    "dayjs": "1.11.10",
    "lodash-es": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3",
    "styled-components": "5.3.9",
    "valtio": "1.11.2"
  },
  "devDependencies": {
    "@testing-library/react": "12.1.2",
    "@types/node": "14.18.9",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-define-config": "1.11.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "postcss": "8.4.23",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.16.0"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
